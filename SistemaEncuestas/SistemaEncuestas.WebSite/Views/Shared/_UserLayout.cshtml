<!DOCTYPE html>
<html>
<head>
    <title>@ViewBag.Title - Poll System @ViewBag.NombreEmpresa</title>

    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="Sistema de gestión de encuestas.">
    <link href="~/Content/images/poll.png" rel="icon" type="image/png" />

    <link href='http://fonts.googleapis.com/css?family=Roboto:400italic,300,400,600,700,800' rel='stylesheet' type='text/css' />
    <link type="text/css" rel="stylesheet" href="~/Content/bootstrap/bootstrap.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/core/Site.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/core/util.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/font-awesome/font-awesome.min.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/material-design-iconic/material-design-iconic-font.min.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/libs/rickshaw/rickshaw.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/libs/morris/morris.core.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/libs/datatables/extensions/dataTables.colVis.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/libs/datatables/extensions/dataTables.tableTools.css" />



    <script src="~/Scripts/libs/jquery/jquery-3.1.1.min.js"></script>
    <script src="~/Scripts/libs/validation/jquery.validate.min.js"></script>
    <script src="~/Scripts/libs/validation/jquery.validate.unobtrusive.min.js"></script>

    @*Librerias reportes*@
    <script src="~/Scripts/libs/chart/Chart.js"></script>
    <link type="text/css" rel="stylesheet" href="~/Content/libs/datatables/jquery.dataTables.css" />
    <link type="text/css" rel="stylesheet" href="~/Content/libs/datatables/jquery.dataTables.buttons.min.css" />
    <script type="text/javascript" language="javascript" src="~/Scripts/jquery/jquery-1.12.4.js"></script>
    <script src="~/Scripts/libs/datatables/jquery.dataTables.min-1.10.15.js"></script>

    <script src="~/Scripts/libs/datatables/dataTables.buttons.min.js"></script>
    <script src="~/Scripts/libs/datatables/buttons.flash.min.js"></script>
    <script src="~/Scripts/libs/datatables/jszip.min.js"></script>
    <script src="~/Scripts/libs/datatables/pdfmake.min.js"></script>
    <script src="~/Scripts/libs/datatables/vfs_fonts.js"></script>
    <script src="~/Scripts/libs/datatables/buttons.html5.min.js"></script>
    <script src="~/Scripts/libs/datatables/buttons.print.min.js"></script>

    <!--[if lt IE 9]>
    <script type="text/javascript" src="~/Scripts/libs/utils/html5shiv.js"></script>
    <script type="text/javascript" src="~/Scripts/libs/utils/respond.min.js"></script>
    <![endif]-->

</head>
<body class="menubar-hoverable header-fixed">
    <header id="header">
        <div class="headerbar">
            <div class="headerbar-left">
                <ul class="header-nav header-nav-options">
                    <li class="header-nav-brand">
                        <div class="brand-holder">
                            <a href="@Url.Action("MisEncuestas", "Encuesta", new { idEmpresa = ViewBag.IdEmpresa })">
                                <img src="@Url.Content("~/Content/images/poll.png")" alt="Logo sistema encuestas" />
                                <span class="text-lg text-bold text-primary">
                                    POLL SYSTEM @ViewBag.NombreEmpresa
                                </span>
                            </a>
                        </div>
                    </li>
                    <li>
                        <a class="btn btn-icon-toggle menubar-toggle" data-toggle="menubar" href="javascript:void(0);">
                            <i class="fa fa-bars"></i>
                        </a>
                    </li>
                </ul>
            </div>
            <div class="headerbar-right">
                <ul class="header-nav header-nav-profile">
                    <li class="dropdown">
                        <a href="javascript:void(0);" class="dropdown-toggle ink-reaction" data-toggle="dropdown">
                            <img src="~/Content/images/user1.png" alt="" />
                            <span class="profile-info">
                                @ViewBag.NombreUsuario
                                <small>@ViewBag.NombreRol</small>
                            </span>
                        </a>
                        <ul class="dropdown-menu animation-dock">
                            <li class="dropdown-header">
                                Menú
                            </li>
                            <li>
                                <a href="javascript:void(0);">Configuración</a>
                            </li>
                            <li class="divider"></li>
                            <li>
                                <a href="@Url.Action("Login", "Usuario")">
                                    <i class="fa fa-fw fa-power-off text-danger"></i> Cerrar sesión
                                </a>
                            </li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
    </header>
    <div id="base">
        <div id="content">
            @RenderBody()
        </div>
        <div id="menubar" class="menubar-inverse ">
            <div class="menubar-fixed-panel">
                <div>
                    <a class="btn btn-icon-toggle btn-default menubar-toggle" data-toggle="menubar" href="javascript:void(0);">
                        <i class="fa fa-bars"></i>
                    </a>
                </div>
                <div class="expanded">
                    <a href="@Url.Action("MisEncuestas", "Encuesta", new { idEmpresa = ViewBag.IdEmpresa })">
                        <span class="text-lg text-bold text-primary ">SISTEMA ENCUESTAS @ViewBag.NombreEmpresa</span>
                    </a>
                </div>
            </div>
            <div class="menubar-scroll-panel">
                <ul id="main-menu" class="gui-controls">
                    <li>
                        <a href="@Url.Action("MisEncuestas", "Encuesta", new { idEmpresa = ViewBag.IdEmpresa })">
                            <div class="gui-icon"><i class="fa fa-table"></i></div>
                            <span class="title">Mis encuestas</span>
                        </a>
                    </li>
                    <li>
                        <a href="@Url.Action("Crear", "Encuesta", new { idEmpresa = ViewBag.IdEmpresa })">
                            <div class="gui-icon"><i class="fa fa-list-alt"></i></div>
                            <span class="title">Crear encuesta</span>
                        </a>
                    </li>
                </ul>
                <div class="menubar-foot-panel">
                    <small class="no-linebreak hidden-folded">
                        <span class="opacity-75">Copyright &copy; 2017</span> <strong>Sistema de encuestas</strong>
                    </small>
                </div>
            </div>
        </div>
    </div>

    <script src="~/Scripts/libs/jquery/jquery-migrate-3.0.0.min.js"></script>
    <script src="~/Scripts/libs/jquery-ui/jquery-ui.min.js"></script>
    <script src="~/Scripts/libs/bootstrap/bootstrap.min.js"></script>
    <script src="~/Scripts/libs/nanoscroller/jquery.nanoscroller.min.js"></script>
    <script src="~/Scripts/libs/autosize/jquery.autosize.min.js"></script>
    <script src="~/Scripts/libs/toastr/toastr.js"></script>
    <script src="~/Scripts/libs/spin/spin.js"></script>
    <script src="~/Scripts/libs/moment/moment.js"></script>
    <script src="~/Scripts/libs/flot/jquery.flot.min.js"></script>
    <script src="~/Scripts/libs/flot/jquery.flot.time.min.js"></script>
    <script src="~/Scripts/libs/flot/jquery.flot.resize.min.js"></script>
    <script src="~/Scripts/libs/flot/jquery.flot.orderBars.js"></script>
    <script src="~/Scripts/libs/flot/jquery.flot.pie.js"></script>
    <script src="~/Scripts/libs/flot/curvedLines.js"></script>
    <script src="~/Scripts/libs/jquery-knob/jquery.knob.min.js"></script>
    <script src="~/Scripts/libs/sparkline/jquery.sparkline.min.js"></script>
    <script src="~/Scripts/libs/d3/d3.min.js"></script>
    <script src="~/Scripts/libs/d3/d3.v3.js"></script>
    <script src="~/Scripts/libs/rickshaw/rickshaw.min.js"></script>
    <script src="~/Scripts/libs/datatables/jquery.dataTables.min.js"></script>
    <script src="~/Scripts/libs/datatables/extensions/ColVis/js/dataTables.colVis.min.js"></script>
    <script src="~/Scripts/libs/datatables/extensions/TableTools/js/dataTables.tableTools.min.js"></script>
    <script src="~/Scripts/libs/datatables/jquery.dataTables.buttons.min.js"></script>
    <script src="~/Scripts/libs/datatables/jquery.dataTables.buttons.flash.min.js"></script>
    <script src="~/Scripts/libs/datatables/jquery.dataTables.jszip.min.js"></script>
    <script src="~/Scripts/libs/datatables/jquery.dataTables.pdfmake.min.js"></script>
    <script src="~/Scripts/libs/datatables/jquery.dataTables.vfs_fonts.js"></script>
    <script src="~/Scripts/libs/datatables/jquery.dataTables.buttons.html5.min.js"></script>
    <script src="~/Scripts/libs/datatables/jquery.dataTables.buttons.print.min.js"></script>
    <script src="~/Scripts/libs/clipboard/clipboard.js"></script>

    <script src="~/Scripts/core/source/App.js"></script>
    <script src="~/Scripts/core/source/AppCard.js"></script>
    <script src="~/Scripts/core/source/AppForm.js"></script>
    <script src="~/Scripts/core/source/AppNavigation.js"></script>
    <script src="~/Scripts/core/source/AppNavSearch.js"></script>
    <script src="~/Scripts/core/source/AppVendor.js"></script>

    <script src="~/Scripts/core/demo/Demo.js"></script>

    <script src="~/Scripts/custom/ValidacionesCliente.js"></script>
    <script src="~/Scripts/custom/Mensajes.js"></script>
    <script src="~/Scripts/custom/VisualizarEncuesta.js"></script>

    @RenderSection("scripts", required: false)
</body>
</html>